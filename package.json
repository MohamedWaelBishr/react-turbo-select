{
  "name": "react-turbo-select",
  "version": "1.1.1",
  "description": "Turbo Select is a flexible and feature-rich React component that provides a convenient dropdown select menu for your applications. Whether you need a simple single selection or a more complex multiple selection with tags, Turbo Select has got you covered. It offers various customization options, making it suitable for a wide range of use cases.",
  "license": "MIT",
  "author": "Mohamed Wael Bishr",
  "keywords": [
    "TurboSelect",
    "React",
    "component",
    "dropdown",
    "select",
    "menu",
    "single selection",
    "multiple selection",
    "tags",
    "customization",
    "use cases",
    "flexible",
    "feature-rich",
    "convenient",
    "options",
    "array",
    "TypeScript",
    "keywords",
    "wide range"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn  eslint",
      "yarn  format"
    ]
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/react-turbo-select.es.js",
      "require": "./dist/react-turbo-select.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css",
    "./dist/index.d.ts": "./dist/index.d.ts"
  },
  "types": "./dist/index.d.ts",
  "type": "module",
  "main": "./dist/react-turbo-select.umd.js",
  "module": "./dist/react-turbo-select.es.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/MohamedWaelBishr/react-turbo-select.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npm run type",
    "type": "echo 'import React from \"react\";\\nimport { TurboSelectProps } from \"./types/types\";\\nexport declare const TurboSelect: React.FC<TurboSelectProps>;\\ndeclare module \"react-turbo-select\"' > dist/index.d.ts",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "react-turbo-select": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.4.4",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.45.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hooks": "^1.0.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.0",
    "vite-plugin-dts": "^3.3.1",
    "vite-plugin-linter": "^2.0.2",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2020": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "sourceType": "module"
    }
  }
}
